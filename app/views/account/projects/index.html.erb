<h1><%= link_to 'New Project', new_account_project_path %></h1>
<% @projects.each do |project| %>
<p>Project name: <%= "#{project.name}"  %>
<%= link_to 'show', account_project_path(project)%>
<%= link_to 'edit', edit_account_project_path(project)%>
<%= link_to 'delete', account_project_path(project), :method => :delete, 
  data: {confirm: "Are you sure?"}%>
</p>
<% end %>